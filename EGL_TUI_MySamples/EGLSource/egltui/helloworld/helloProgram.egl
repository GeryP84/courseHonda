package egltui.helloworld;

// text UI program
//
program helloProgram type TextUIProgram
	
	use helloGroup;
	
	function main()
		while (converseVar.eventKey not pf3)
			converse HelloWorld1;
			if(converseVar.eventKey is pf5)
				move HelloWorld1.InputField to HelloWorld2.OutputField2;
				set HelloWorld2.OutputField2 protect;
				converse HelloWorld2;
			end
		end
	end 
	
end
